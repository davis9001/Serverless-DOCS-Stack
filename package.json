{
  "name": "docs-stack-starter",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build && wrangler deploy .svelte-kit/cloudflare/_worker.js --site ./.cloudflare/public && wrangler tail",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "db:migrate": "drizzle-kit generate",
    "db:push:dev": "wrangler d1 migrations apply test-DOCS --local",
    "db:push:prod": "wrangler d1 migrations apply test-DOCS --remote",
    "db:studio:dev": "ENV=dev drizzle-kit studio",
    "db:studio:prod": "cross-env NODE_ENV=production drizzle-kit studio",
    "db:backup:prod": "wrangler d1 export test-DOCS --output backup-prod.sql --remote",
    "test": "vitest"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241018.0",
    "@faker-js/faker": "^8.4.1",
    "@sveltejs/adapter-cloudflare": "^4.7.3",
    "@sveltejs/kit": "^2.7.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.8",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint": "8.56.10",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.26.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "2.38.0",
    "pino-pretty": "^11.3.0",
    "postcss": "^8.4.47",
    "postcss-load-config": "^5.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "svelte": "5.0.0-next.123",
    "svelte-check": "^3.8.6",
    "sveltekit-flash-message": "^2.4.4",
    "sveltekit-rate-limiter": "^0.5.2",
    "sveltekit-view-transition": "^0.5.3",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vite-plugin-kit-routes": "^0.6.11",
    "vite-plugin-top-level-await": "^1.4.4",
    "vitest": "^1.6.0",
    "wrangler": "~3.57.2"
  },
  "type": "module",
  "dependencies": {
    "@libsql/client": "^0.6.2",
    "@lucia-auth/adapter-sqlite": "^3.0.2",
    "@noble/hashes": "^1.5.0",
    "@sveltejs/adapter-cloudflare-workers": "^2.5.5",
    "arctic": "^1.9.2",
    "better-sqlite3": "^10.1.0",
    "bits-ui": "^0.21.16",
    "check-password-strength": "^2.0.10",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "drizzle-orm": "^0.35.2",
    "formsnap": "^1.0.1",
    "lucia": "^3.2.1",
    "lucide-svelte": "^0.378.0",
    "mode-watcher": "^0.3.1",
    "oslo": "^1.2.1",
    "pg": "^8.13.0",
    "pino": "^9.5.0",
    "resend": "^3.5.0",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.20.0",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.2.1",
    "zod": "^3.23.8"
  }
}
